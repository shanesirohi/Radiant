<body>
    <img src="img/wave(1).png" alt="" class="wave">
    <div class="container">
        <div class="img">
            <img src="https://raw.githubusercontent.com/nareshkumaralaria/Responsive-Registration-Login-Form/7734fe527533cbce0aedfe389a7a5e180cd95286/img/login.svg" alt="">
        </div>
        <div class="login-container">
            <form action="/register" method="POST">
                <img src="img/female avtar.svg" class="avtar" alt="">
                <h2>SignUp to Continue</h2>
                <div class="input-div one">
                    <div class="i">
                        <i class="fas fa-user"></i>
                    </div>
                    <div>
                        <h5></h5><br>
                        <input name='username' placeholder='Username' type="text" class="input">
                    </div>
                </div>
                <div class="input-div two">
                    <div class="i">
                        <i class="fas fa-lock"></i>
                    </div>
                    <div>
                        <h5></h5><br>
                        <input name='password' placeholder='Password' type="password" class="input">
                    </div>
                </div>
                <div class="input-div two">
                    <div class="i">
                        <i class="fas fa-user"></i>
                    </div>
                    <div>
                        <h5></h5><br>
                        <input name='college' placeholder='Name of your school [SAJS, AMITY, DPS]' type="text" class="input">
                    </div>
                </div>
                <div class="input-div two">
                    <div class="i">
                        <i class="fas fa-user"></i>
                    </div>
                    <div>
                        <h5></h5><br>
                        <input name='musicGenre' placeholder='Your favourite music genre[Pop, Classical, Rock, Jazz, Metal]' type="text" class="input">
                    </div>
                </div>
                <a href="#">Forgot Password</a>
                <a href="/login">Already have an account? Login here</a>
                <button class="btn">Signup</button>
            </form>
        </div>
    </div>
    <script src="js/javascript.js" type="text/javascript"></script>
